{
  "name": "multitape-tm-simulator",
  "version": "1.0.0",
  "description": "Simulates an arbitrary number of tapes for a turing machine",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js --mode development",
    "start": "webpack serve --mode development",
    "test:build": "cross-env NODE_ENV=test npx webpack --config webpack-test.config.js --mode development",
    "test:watch": "cross-env NODE_ENV=test npx webpack --config webpack-test.config.js --mode development --watch",
    "test": "npm run test:build && jasmine-browser-runner runSpecs",
    "test:serve": "npm run test:build && jasmine-browser-runner"
  },
  "keywords": [
    "tm",
    "simulator"
  ],
  "author": "Gabrielle B Zhu",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/register": "^7.23.7",
    "@testing-library/react": "^14.2.1",
    "@types/jasmine": "^5.1.4",
    "@types/react": "^18.2.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jasmine": "^5.1.0",
    "jasmine-browser-runner": "^2.3.0",
    "jasmine-core": "^5.1.2",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "3.0.3",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}